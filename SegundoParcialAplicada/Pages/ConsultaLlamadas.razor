@page "/cConsulta"
<h3>Consulta de Llamadas</h3>

<table class="table">
    <thead>
        <tr>
            <td>Id</td>
            <td>Descripcion</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in list)
        {
            <tr>
                <td>@item.CallId</td>
                <td>@item.Descripcion</td>
                <td><a class="btn btn-sm btn-outline-primary" href="rllamadas/@item.CallId">Ver</a></td>
            </tr>
        }
    </tbody>
</table>



@code {

    CallsController controller = new CallsController();

    List<Calls> list;
    protected override void OnInitialized()
    {
        list = controller.GetList(e => true);
    }



}